{
  "manifest_version": 3,
  "name": "Art Wallpaper New Tab",
  "version": "1.0.0",
  "description": "Replace new tab with beautiful art wallpaper that changes daily",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
